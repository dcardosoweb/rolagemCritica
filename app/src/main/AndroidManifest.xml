<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.dcardoso.rolagemcritica">

    <application
        android:name=".CardCriticoApp"
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".ConfiguracaoActivity"></activity>
        <activity android:name=".CreditoUsoImagemActivity" />
        <activity android:name=".CreditosDungeonsDragonsActivity"></activity>
        <activity android:name=".CreditosRpgNextActivity" />
        <activity android:name=".SobreActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>